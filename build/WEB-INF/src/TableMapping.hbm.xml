<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.machine.model.Name" table="Names">
		<id name="id" type="int" column="NameID">
			<generator class="increment" />
		</id>
		
		<property name="firstName" type="string">
			<column name="NameFirstName"></column>
		</property>
		
		<property name="lastName" type="string">
			<column name="NameLastName"></column>
		</property>
		
		<property name="ethnicity" type="string">
			<column name="NameEthnicity"></column>
		</property>
		
	</class>
</hibernate-mapping>